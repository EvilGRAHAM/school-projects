cv.adaptive.glmnet <-
function(
x
,y
,alpha = 1
,gamma = 1
,weights
,nfolds = 10
,parallel = FALSE
,...
){
cv.alasso.ridge <-
cv.glmnet(
x = x
,y = y
,alpha = 0
,nfolds = nfolds
,parallel = parallel
)
cv.alasso.weights <- 1 / abs(coef(object = cv.alasso.ridge, s = "lambda.min", exact = TRUE)[-1, 1])^(gamma)
cv.alasso.model <-
cv.glmnet(
x = x
,y = y
,alpha = alpha
,penalty.factor = cv.alasso.weights
)
cv.alasso.model
}
